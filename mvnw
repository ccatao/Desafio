# ----------------------------------------------------------------------------
# Maven Start Up Batch script
# ----------------------------------------------------------------------------

set -e

MVNW_REPOURL="https://repo.maven.apache.org/maven2"
MVNW_USERNAME=""
MVNW_PASSWORD=""

if [ -f .mvn/jvm.config ]; then
  JVM_CONFIG=.mvn/jvm.config
fi

if [ -f "$JVM_CONFIG" ]; then
  if [ -r "$JVM_CONFIG" ]; then
    . "$JVM_CONFIG"
  fi
fi

exec java $MAVEN_OPTS -classpath "$MAVEN_PROJECTBASEDIR/.mvn/wrapper/maven-wrapper.jar" "-Dmaven.multiModuleProjectDirectory=$MAVEN_PROJECTBASEDIR" \
  org.apache.maven.wrapper.MavenWrapperMain "$@"